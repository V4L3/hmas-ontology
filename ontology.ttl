###  http://example.org/ontology#source
:source rdf:type owl:DatatypeProperty ;
        rdfs:domain :LogicalTimestamp ;
        rdfs:range xsd:string ;
        rdfs:comment "The source of the logical timestamp" ;
        rdfs:label "Source" .


###  http://example.org/ontology#value
:value rdf:type owl:DatatypeProperty ;
       rdfs:domain :LogicalTimestamp ;
       rdfs:range xsd:int ;
       rdfs:comment "The value of the logical timestamp" ;
       rdfs:label "Value" .


